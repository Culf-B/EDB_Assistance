{
	"manifest_version":3,
	"version":"1.0",
	"name":"EDB Assistance",
	"content_scripts":[
	  {
	   "matches":["*://*.borger.dk/*"],
	   "js":["main.js"]
	  }
	],
	"icons": {
		"48": "icon.png",
		"96": "icon.png"
	},
	"browser_action":{
		"default_icon":{
		  "19":"icon.png",
		  "38":"icon.png"
		}
	},
	"background":{
        "scripts":["background.js"]
	},
	"permissions":[
		"tabs"
	],
	"web_accessible_resources":[
		"help.html"
	]
}
